<% if @error_message %>
	<div class="form-error">
		<%= @error_message %>
	</div>
<% end %>

<%= form_tag("/login") do %>
	<p>User</p>
	<input type="text" name="name" required="required">
	<p>PassWord</p>
	<input type="password" name="password" required="required">
	<input type="submit" value="login">
<% end %>


